{
  "name": "outfitter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "auth0-js": "^9.0.2",
    "flow-bin": "^0.60.1",
    "husky": "^0.14.3",
    "jwt-decode": "^2.2.0",
    "lint-staged": "^6.0.0",
    "prettier": "^1.8.2",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^5.0.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.77.0",
    "source-map-explorer": "^1.6.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/{registerServiceWorker,index}.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 65,
        "functions": 66,
        "lines": 67,
        "statements": 0
      }
    }
  },
  "scripts": {
    "precommit": "lint-staged",
    "flow": "flow",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "fetch-mock": "^5.13.1",
    "local-ssl-proxy": "^1.3.0",
    "redux-devtools": "^3.5.0",
    "redux-mock-store": "^1.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
